<html>
  <head>
    <title>Mastercard</title>

    <style>
      body {
        background-color: #fcc;
        -moz-user-select: none;
      }
    </style>
    <script type="text/javascript" language="javascript">
      <!--

      function __$(id){
        return document.getElementById(id);
      }

      //-->
    </script>
  </head>
  <body>
    <table width="100%" border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td colspan="3">
          <%= render(:partial => "mastercard_header" ) %>
        </td>
      </tr>
      <tr class ="page1" style="display: block">
        <td  style="vertical-align: top;">
          <%= render(:partial => "patient_details" ) %>
        </td>
        <td colspan="2" style="vertical-align: top;" >
          <%= render(:partial => "status_details" ) %>
        </td>
      </tr>
      <tr class ="page1" style="display: block;">
        <td style="vertical-align: top;">
          <%= render(:partial => "rapid_test" ) %>
        </td>
        <td colspan="2" style="vertical-align: top;" >
          <%= render(:partial => "dna_test" ) %>
        </td>

      </tr>
      <tr width="100%" class ="page1" style="display: block;width: 100%;">
        <td colspan="3" style="vertical-align: top; padding: 0px; margin: 0px; width: 1430px; max-width: 95%" >
          <%= render(:partial => "notes_lines" ) %>
        </td>
      </tr>      
      <tr class ="page2" style="display: none;">
        <td colspan="3" style="width: 100%">
          <%= render(:partial => "mastercard_details_#{@type}" ) %>
        </td>
      </tr>
    </table>
   

  </body>
</html>