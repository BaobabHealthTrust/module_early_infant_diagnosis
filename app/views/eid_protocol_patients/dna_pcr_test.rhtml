<form method='post' action='/encounters/create'>
	<input type='hidden' name='program' value='EARLY INFANT DIAGNOSIS PROGRAM' />
	<input type='hidden' name='encounter_type' value='DNA-PCR TEST ' />
	<input type='hidden' name='patient_id' value='<%= @patient.id %>' />
	<input type='hidden' name='user_id' value='<%= session[:user_id] %>' />
	<table>
		<tr>
			<td>
				DNA-PCR Testing Sample Date 
			</td>
			<td>
<input type='text' id='1.1.1'  name='concept[DNA-PCR Testing Sample Date ]' helpText='DNA-PCR Testing Sample Date '  pos='0'  field_type='date' />
			</td>
		</tr>
		<tr>
			<td>
				DNA-PCR Testing Sample ID 
			</td>
			<td>
<input type='text' id='1.1.2'  name='concept[DNA-PCR Testing Sample ID ]' helpText='DNA-PCR Testing Sample ID '  pos='1' />
			</td>
		</tr>
		<tr>
			<td>
				DNA-PCR Testing Result received Date 
			</td>
			<td>
<input type='text' id='1.1.3'  name='concept[DNA-PCR Testing Result received Date ]' helpText='DNA-PCR Testing Result received Date '  pos='2'  field_type='date' />
			</td>
		</tr>
		<tr>
			<td>
				DNA-PCR Testing Result 
			</td>
			<td>
<select id='1.1.4' name='concept[DNA-PCR Testing Result ]' helpText='DNA-PCR Testing Result ' pos='3' >
<option></option>
<option value='Positive'>Positive</option>
 <option value='Negative'>Negative</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				DNA-PCR Testing Result given Date 
			</td>
			<td>
<input type='text' id='1.1.5'  name='concept[DNA-PCR Testing Result given Date ]' helpText='DNA-PCR Testing Result given Date '  pos='4'  field_type='date' />
			</td>
		</tr>
		<tr>
			<td>
				DNA-PCR Testing Result given Age (months) 
			</td>
			<td>
<input type='text' id='1.1.6'  name='concept[DNA-PCR Testing Result given Age]' helpText='DNA-PCR Testing Result given Age (months) '  absoluteMax='<%= @patient.age_in_months + 1%>'  pos='5'  tt_pageStyleClass='NumbersOnlyWithUnknown'  field_type='number' />
			</td>
		</tr>
	</table>
</form>
